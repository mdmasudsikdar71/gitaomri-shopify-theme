* {
    margin: 0;
    padding: 0;
}

a,
h1,
h2,
h3,
h4,
h5,
h6,
p,
ul,
li,
button {
    padding: 0;
    margin: 0;
    text-decoration: none;
    border: 0;
    background: none;
    list-style-type: none;
}

body {
    background: #fff;
    overflow-x: hidden;
    font-family: "times new roman", times, serif;
}

.container {
    max-width: 1000px;
    margin: 0 auto;
}

.d_flex{
    display: flex;
    align-items: center;
}

:root{
    --main_color1: #151515;
    --main_color2: #575757;
    --hover_color: #fde681;
    --bg_color: #FAD94E;
    --text_color: #4F4F4F;
    --white_color: #fff;
}


h1{
    font-size: 77px;
    line-height: 77px;
    font-weight: 700;
    color: #A70757;
    font-family: "playfair display", serif;
}

h3{
    font-size: 30px;
    line-height: 40px;
    font-weight: 700;
}

a {
    font-size: 14px;
    line-height: 25px;
    font-weight: 400;
    color: var(--text_color);
    transition: all linear .2s;
    display: inline-block;
    font-family: 'Lato', sans-serif;
}

a:hover{
    color: var(--hover_color);
}

p{
    font-size: 15px;
    line-height: 24px;
    font-weight: 400;
    font-style: italic;
}

/* ---------------------------------------------------------------------------------------------------------------------------
    Logo
--------------------------------------------------------------------------------------------------------------------------- */

.logoImg{
    text-align: center;
    position: relative;
}

.logoImg img{
    height: 195px;
}

.Bag {
    position: absolute;
    top: 50%;
    right: 0;
    transform: translateY(-50%);
}

.Bag img {
    height: 40px;
}

.Bag span {
    font-size: 18px;
    line-height: 20px;
    font-weight: 500;
    color: var(--white_color);
    position: absolute;
    top: 18px;
    left: 50%;
    transform: translateX( -50%);
    font-style: italic;
}

/* ---------------------------------------------------------------------------------------------------------------------------
    MenuBar
--------------------------------------------------------------------------------------------------------------------------- */

.MenuBar {
    margin-top: -20px;
    z-index: 9;
    position: relative;
}

.MenuItem {
    text-align: center;
    padding-bottom: 90px;
}

.MenuItem ul li{
    display: inline-block;
    position: relative;
    padding: 5px 40px;
}

.MenuItem a{
    text-transform: uppercase;
    font-family: lato-light, lato, sans-serif;
}

.subMenu {
    position: absolute;
    top: 100%;
    left: 0;
    text-align: left;
    width: 230px;
    transition: all linear .2s;
    opacity: 0;
    visibility: hidden;
}

.subMenu li{
    display: block;
    text-align: left;
}

.MenuItem ul li:hover .subMenu{
    opacity: 1;
    visibility: visible;
}

.Logo{
    border-bottom: 2px solid var(--text_color);
}

/* MobileMenu */

.MobileMenu {
    display: none;
}




/* RightSideBar */
.RightSideBar .offcanvas-header{
    background: #151515;
    padding: 40px 30px;
    position: relative;
    justify-content: center;
}

.RightSideBar .offcanvas-header button{
    color: var(--white_color);
}

.RightSideBar .offcanvas-header button svg{
    position: absolute;
    left: 30px;
    top: 50%;
    transform: translateY(-50%);
}

.RightSideBar .offcanvas-header h5 {
    text-align: center;
    color: var(--white_color);
    font-size: 24px;
    font-style: italic;
    line-height: 35px;
    font-weight: 400;
}

.RightSideBar .offcanvas-body{
    padding: 0px;
    text-align: left;
}

.RightSideBar div#offcanvasRight {
    width: 355px;
}

.RightSideBar .offcanvas-body .img{
    margin-right: 15px;
}

.RightSideBar .offcanvas-body img{
    height: 90px;
    object-fit: fill;
}

.RightSideBarCart h5{
    font-size: 16px;
    line-height: 24px;
    font-style: italic;
    color: var(--main_color2);
    font-weight: 400;
}

.RightSideBarCart p{
    font-size: 16px;
    line-height: 24px;
    font-style: italic;
    color: var(--main_color1);
    font-weight: 700;
    margin-top: 2px;
}

.RightSideBarCart .input{
    margin-top: 10px;
    width: 70px;
    position: relative;
}

.RightSideBarCart .input .minus, .RightSideBarCart .input .plus{
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    cursor: pointer;
}

.RightSideBarCart .input .minus{
    left: 9px;
}

.RightSideBarCart .input .plus{
    right: 9px;
}

.RightSideBarCart .input img{
    height: 9px;
}

.RightSideBarCart .input input{
    width: 100%;
    text-align: center;
    font-size: 16px;
    line-height: 24px;
    font-style: italic;
    color: var(--main_color1);
    font-weight: 400;
    margin-top: 2px;
    padding: 2px 5px;
    border: 1px solid var(--main_color1);
}

.RightSideBarCart .input input:focus{
    outline: unset;
}

.RightSideBarItem{
    display: flex;
    position: relative;
    border-bottom: 1px solid #ddd;
    padding: 30px;
}

.RightSideBarItem .text{
    width: 80%;
}

.RightSideBarItem .closeHover{
    position: absolute;
    top: 30px;
    right: 30px;
    width: 18px;
    height: 18px;
    border-radius: 50%;
    background: var(--white_color);
    border: 1px solid #DDD;
    padding: 5px;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
    transition: all linear .2s;
    opacity: 0;
}

.RightSideBarItem:hover .closeHover{
    opacity: 1;
}

.RightSideBarItem .closeHover img{
    height: 6px;
}

.RightSideBarCart {
    height: 64%;
    overflow-y: auto;
}

.RightSideBar ::-webkit-scrollbar {
    display: none;
}

.RightSideBar  .subtotal{
    margin-top: 20px;
    padding: 0 30px;
    padding-bottom: 30px;
    border-bottom: 1px solid #ddd;
}

.RightSideBar .subtotal h4{
    font-size: 26px;
    line-height: 35px;
    color: var(--main_color1);
    font-weight: 400;
    font-style: italic;
}

.RightSideBar .subtotal h5{
    font-size: 28px;
    line-height: 38px;
    color: var(--main_color1);
    font-weight: 400;
    font-style: italic;
    margin-top: 5px;
}

.RightSideBar  .viewCart{
    padding: 30px;
    text-align: center;
}

.RightSideBar  .viewCart a{
    background: #FAD94E;
    padding: 8px;
    font-size: 14px;
    line-height: 24px;
    color: var(--white_color);
    font-weight: 400;
    font-style: italic;
    width: 100%;
    font-family: "times new roman", times, serif;
}

/* ---------------------------------------------------------------------------------------------------------------------------
    Banner
--------------------------------------------------------------------------------------------------------------------------- */

.Banner {
    background: #EBEBEB;
    align-items: self-start;
}

.BannerContent {
    background: #EBEBEB;
    padding: 80px 0;
    width: 50%;
    text-align: center;
}

.BannerContent h1{
    max-width: 50%;
    margin: 0 auto;
    text-align: center;
}

.BannerContent h1 span{
    font-size: 110px;
}

.BannerContent h3 {
    color: #A70757;
    margin-top: 30px;
    font-family: raleway, sans-serif;
}

.BannerContent span{
    font-size: 18px;
}

.BannerContent a {
    font-size: 14px;
    {% comment %}font-weight: normal;{% endcomment %}
    font-style: italic;
    color: #A70757;
    margin-top: 80px;
    text-transform: uppercase;
    background: #EBEBEB;
    border: 1px solid #A70757;
    border-radius: 30px;
    padding: 10px 30px;
    font-weight: 400;
    font-family: "times new roman", times, serif;
}

.BannerContent a:hover{
    background: #B33C00;
    border: 1px solid var(--hover_color);
    color: var(--white_color);
}

.BanenrImg{
    width: 50%;
    height: 100%;
}

.BanenrImg img{
    width: 100%;
}

/* ---------------------------------------------------------------------------------------------------------------------------
    Product
--------------------------------------------------------------------------------------------------------------------------- */
.Product{
    padding: 30px 40px;
}

.SliderItem{
    position: relative;
}

.SliderItem span {
    font-size: 13px;
    font-weight: 400;
    line-height: 20px;
    padding: 3px 10px;
    font-style: italic;
    background: var(--bg_color);
    color: var(--white_color);
    position: absolute;
    top: 0;
    left: 0;
    font-family: "times new roman", times, serif;
}

.SliderItem:hover  .img .ImgOne{
    display: none;
}

.SliderItem .img .ImgTwo{
    display: none;
}

.SliderItem:hover  .img .ImgTwo{
    display: block;
}

.SliderItem img{
    height: 100%;
    width: 100%;
}

.SliderItem .text{
    margin-top: 15px;
    text-align: center;
}

.SliderItem a{
    width: 100%;
    overflow: hidden;
}

.SliderItem h3{
    font-size: 16px;
    line-height: 22px;
    font-weight: 400;
    font-style: italic;
    position: relative;
    padding-bottom: 10px;
    margin-bottom: 10px;
    color: var(--main_color1);
    font-family: "times new roman", times, serif;
}

.SliderItem h3::after {
    position: absolute;
    content: '';
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
    width: 20px;
    height: 1px;
    background: var(--main_color1);
}

.SliderItem p {
    font-style: italic;
    font-size: 14px;
    line-height: 18px;
    color: var(--text_color);
    font-weight: 500;
    font-family: "times new roman", times, serif;
}

.SliderItem a:hover{
    color: unset;
}

.SliderItem .img{
    position: relative;
}

.SliderItem .img a{
    width: 100%;
}

.SliderItem .img .quickView{
    background: rgba(255, 255, 255, 0.75);
    padding: 15px;
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    text-align: center;
    transform: translateY(50px);
    transform-origin: top;
    opacity: 0;
    visibility: hidden;
    transition: all linear .2s;
    z-index: 999;
}

.SliderItem .img .quickView button {
    color: var(--main_color1);
    font-weight: 400;
    font-size: 14px;
    line-height: 22px;
    width: 100%;
    font-style: italic;
}

.SliderItem:hover .img .quickView{
    opacity: 1;
    visibility: visible;
    transform: translateY(0px);
}

.Product{
    position: relative;
}

.Product .next, .Product .prev{
    position: absolute;
    top: 44%;
    left: 10px;
    cursor: pointer;
    font-size: 20px;
}

.Product .next {
    right: 10px;
    left: unset;
}

/* Modal */

.modal-dialog {
    max-width: 940px;
    height: 100%;
}

.modal-dialog .btn-close{
    position: absolute;
    top: 15px;
    right: 15px;
    color: var(--main_color1);
    cursor: pointer;
    z-index: 9;
}

.modal-dialog .modal-body {
    padding: 35px;
}

.modal-content {
    border-radius: 0;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.ModalImg img{
    width: 100%;
}

.ModalContent h3 {
    font-size: 26px;
    font-weight: 400;
    line-height: 36px;
    font-style: italic;
    color: var(--main_color2);
}

.ModalContent h4{
    font-size: 18px;
    line-height: 28px;
    font-weight: 300;
    color: var(--main_color2);
    margin-top: 10px;
    font-style: italic;
    letter-spacing: 1px;
}

.ModalFrom{
    margin-top: 20px;
}

.CustomeInput{
    margin-top: 20px;
}

.CustomeInput label{
    display: block;
    font-size: 15px;
    line-height: 22px;
    font-weight: 300;
    color: var(--main_color1);
    font-style: italic;
    margin-bottom: 10px;
}

.CustomeInput input, .CustomeInput textarea, .CustomeInput select {
    width: 100%;
    padding: 10px 15px;
    border-radius: 0px;
    font-style: italic;
    border: 1px solid #DDD;
    font-size: 14px;
    line-height: 24px;
    color: var(--main_color1);
}

.CustomeInput input{
    padding: 6px 15px;
}

.CustomeInput .Quantity{
    width: 100px;
}

.CustomeInput button{
    background: var(--bg_color);
    padding: 10px 30px;
    width: 100%;
    text-align: center;
    margin-top: 20px;
    font-size: 16px;
    font-weight: 400;
    lighting-color: 24px;
    color: var(--white_color);
    font-style: italic;
}

.CustomeInput button:hover{
    background: var(--hover_color);
}

.CustomeInput a{
    font-size: 14px;
    line-height: 22px;
    font-weight: 200;
    color: var(--main_color1);
    font-style: italic;
    letter-spacing: 0;
    text-decoration: underline;
    font-family: "times new roman", times, serif;
}

.CustomeInput .radioInput input{
    width: 20px;
    height: 20px;
    margin-right: 20px;
    cursor: pointer;
    position: relative;
}

.CustomeInput .radioInput input[type='radio']:after {
    width: 17px;
    height: 17px;
    border-radius: 15px;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    position: relative;
    background: var(--main_color2);
    content: '';
    display: inline-block;
    visibility: visible;
}

.CustomeInput .radioInput input.blue[type='radio']:after {
    background: blue;
}

.CustomeInput .radioInput input.red[type='radio']:after {
    background: red;
}

input[type='radio']:checked:after {
    border: 2px solid #DDD;
}

.SocialIcon {
    margin-top: 30px;
}

.SocialIcon a{
    font-size: 14px;
    margin-right: 20px;
}

.SocialIcon a img{
    height: 16px;
}

/* ---------------------------------------------------------------------------------------------------------------------------
    Footer
--------------------------------------------------------------------------------------------------------------------------- */
.Footer {
    padding: 25px 0px;
    border-top: 3px solid var(--main_color1);
}

.Footer .container{
    max-width: 1220px;
}

.Footer li{
    display: inline-block;
