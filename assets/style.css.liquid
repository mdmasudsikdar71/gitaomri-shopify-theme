    color: var(--main_color1);
}

.FooterItem ul li a:hover{
    color: #a4a2a2;
}

.FooterItem ul li a svg{
    margin-right: 30px;
    font-size: 30px;
}

.FooterItem ul li a:last-child svg{
    margin-right: 0;
}

