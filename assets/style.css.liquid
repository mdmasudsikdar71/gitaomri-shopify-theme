/* BlogPart */
.BlogPart{
    margin-top: 50px;
}

.BlogItem{
    position: relative;
}

.BlogPart img{
    width: 100%;
}

.BlogPart .overlay{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, .4);
    padding: 20px;
}

.BlogPart .overlay .header a span{
    font-size: 13px;
    font-weight: 400;
    line-height: 18px;
    color: var(--white_color);
}

.BlogPart .overlay .header a img{
    width: 20px;
    margin-left: 5px;
}

.BlogPart .overlay .header ul li{
    position: relative;
    font-size: 13px;
    font-weight: 400;
    line-height: 18px;
    color: var(--white_color);
    margin-right: 10px;
    padding-right: 10px;
    display: inline-block;
}

.BlogPart .overlay .header ul li::after {
    position: absolute;
    content: "";
    top: 8px;
    right: -5px;
    width: 3px;
    height: 3px;
    background: var(--white_color);
    border-radius: 50%;
}

.BlogPart .overlay .header ul li:last-child::after{
    display: none;
}

.BlogPart .overlay .header {
    height: 68%;
}

.BlogPart .overlay .bottom h4{
    border-bottom: 1px solid var(--white_color);
    padding-bottom: 15px;
    margin-bottom: 15px;
}

.BlogPart .overlay .bottom h4 a {
    font-size: 25px;
    line-height: 35px;
    font-weight: 500;
    color: var(--white_color);
}

.commentPart a{
    margin-right: 20px;
}

.commentPart svg{
    color: var(--white_color);
    font-size: 16px;
}

.commentPart span{
    font-size: 13px;
    font-weight: 400;
    line-height: 18px;
    color: var(--white_color);
    margin-left: 5px;
}

.BlogPart .overlay .commentPart{
    justify-content: space-between;
}

.commentPart .heart{
    margin-right: 0;
}

.commentPart .heart svg{
    color: #E84A43;
}

.commentPart .heart span{
    margin-right: 5px;
}




