/* ---------------------------------------------------------------------------------------------------------------------------
    Contact
--------------------------------------------------------------------------------------------------------------------------- */
.Contact{
    padding: 40px;
}

.ContactImg {
    padding-top: 80px;
    margin-right: 30px;
}

.Contact img{
